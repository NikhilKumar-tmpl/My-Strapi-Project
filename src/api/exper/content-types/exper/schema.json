{
  "kind": "collectionType",
  "collectionName": "expers",
  "info": {
    "singularName": "exper",
    "pluralName": "expers",
    "displayName": "activity card"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "price": {
      "type": "biginteger"
    },
    "rating": {
      "type": "decimal"
    },
    "image": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "tag": {
      "type": "enumeration",
      "enum": [
        "Iconic",
        "Popular",
        "Niche",
        "Top Pick"
      ]
    },
    "category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::category.category",
      "inversedBy": "activity_cards"
    },
    "shortDescription": {
      "type": "string"
    },
    "newDescription": {
      "type": "richtext"
    },
    "location": {
      "type": "component",
      "component": "location.location",
      "repeatable": false
    },
    "WheelchairAccessibility": {
      "type": "component",
      "component": "wheelchair-accessibility.wheelchair-accessibility",
      "repeatable": false
    },
    "tagImage": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "time": {
      "type": "string"
    },
    "my_filters": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::my-filter.my-filter",
      "mappedBy": "activity_cards"
    }
  }
}
